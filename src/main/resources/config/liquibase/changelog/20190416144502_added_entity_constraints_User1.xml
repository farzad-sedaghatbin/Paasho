                                 referencedTableName="event"/>

        <addForeignKeyConstraint baseColumnNames="notification_id"
                                 baseTableName="user_1"
                                 constraintName="fk_user_1_notification_id"
                                 referencedColumnNames="id"
                                 referencedTableName="notification"/>

    </changeSet>
</databaseChangeLog>
